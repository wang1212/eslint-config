{
  "name": "@wang1212/eslint-config",
  "version": "0.1.1",
  "description": "Community-recommended best practices for [ESLint](https://eslint.org/) rule configuration.(Use with [Prettier](https://prettier.io/))",
  "keywords": [
    "eslint-config",
    "prettier"
  ],
  "homepage": "https://github.com/wang1212/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wang1212/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/wang1212/eslint-config/issues"
  },
  "license": "MIT",
  "author": "wang1212",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --config=.eslintrc.cjs --fix --quiet --cache --cache-location=node_modules/.cache/.eslintcache --format=pretty",
    "test": "echo test",
    "build": "echo build",
    "commit": "cz",
    "prerelease": "npm run test",
    "release": "standard-version",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "exports": {
    ".": "./index.js"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.13.0",
    "eslint-formatter-pretty": "4.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "prettier": "2.6.2",
    "standard-version": "9.3.2"
  }
}
